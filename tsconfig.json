// https://github.com/tsconfig/bases/blob/main/bases/node16.json
{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",
    "compilerOptions": {
      "lib": ["es2021", "DOM"],
      "module": "commonjs",
      "target": "ESNext",
      "resolveJsonModule": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": false
    }
  }
  